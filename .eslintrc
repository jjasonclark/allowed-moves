{
  "extends": "prettier",
  "plugins": ["prettier"],
  "env": {
    "browser": true,
    "es6": true,
    "node": false
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "max-len": ["error", {
      "code": 120,
      "ignoreStrings": true
    }],
    "prettier/prettier": "error"
  }
}
